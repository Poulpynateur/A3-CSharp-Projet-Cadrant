<UserControl x:Class="EasySave.View.Log"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EasySave"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Margin="5" Orientation="Horizontal">
        <Image Visibility="Collapsed" x:Name="LogTextIconCaution" Source="../assets/Caution.png" Height="15" Margin="0,0,10,0"/>
        <Image Visibility="Collapsed" x:Name="LogTextIconInfo" Source="../assets/Info.png" Height="15" Margin="0,0,10,0"/>
        <Image Visibility="Collapsed" x:Name="LogTextIconSuccess" Source="../assets/Success.png" Height="15" Margin="0,0,10,0"/>
        <TextBlock x:Name="LogText" Grid.Row="4" Grid.ColumnSpan="3" VerticalAlignment="Center"></TextBlock>
    </StackPanel>
</UserControl>
